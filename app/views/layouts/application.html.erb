<!DOCTYPE html>
<html>
<head>
  <title><%= ApplicationHelper::page_title yield :page_title %></title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="row">
  <div id="menu">
    <%= render "layouts/menu" %>
  </div>
  <div id="content">
    <div id="title"><%= yield :title %></div>

    <%= render 'layouts/flashs' %>

    <%= yield %>
  </div>
</div>

<%= debug(params) if Rails.env.development? %>
</body>
</html>
